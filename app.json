{
  "expo": {
    "name": "EDM Prompter",
    "slug": "edm-prompter",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6A1B9A"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.edmprompter"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yourname.edmprompter"
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      "expo-sqlite"
    ],

    "extra": {
      "eas": {
        "projectId": "REPLACE_WITH_EAS_PROJECT_ID"
      }
    },

    "runtimeVersion": {
      "policy": "sdkVersion"
    },

    "sdkVersion": "50.0.0"
  }
}
